{
  "applications": {
    "Example.HttpApi.Host": {
      "type": "dotnet-project",
      "launchUrl": "https://localhost:44384",
      "path": "../../../src/Example.HttpApi.Host/Example.HttpApi.Host.csproj",
      "kubernetesService": ".*-httpapihost$",
      "healthCheckEndpoint": "/health-status",
      "healthUiEndpoint": "/health-ui",
      "execution": {
        "order": 4
      }
    }
  },
  "containers": {
    "serviceName": "Example-Containers",
  },
  "tasks":{
    "Initialize Solution":{
    "type": "cli",
    "runOnSolutionOpen": true,
    "startCommand": "etc/scripts/initialize-solution.ps1",
    "workingDirectory": "../../../"
    },
    "Migrate Database":{
    "type": "cli",
    "runOnSolutionOpen": false,
    "startCommand": "etc/scripts/migrate-database.ps1",
    "workingDirectory": "../../../"
    }
  }
}